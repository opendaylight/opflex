module[gbp]
{
    class[EpGroup;
          super=gbp/BehavioralGroup;
          concrete;
          ]
    {
        dependency[network;
                   cardinality=single;
                   ]
        {
            to[class=gbp/ForwardingBehavioralGroup;
                cardinality=many;
                ]
        }

        dependency[provContract;
                   cardinality=many;
                   ]
        {
            to[class=gbp/Contract;
                cardinality=many;
                ]
        }
        dependency[consContract;
                   cardinality=many;
                   ]
        {
            to[class=gbp/Contract;
                cardinality=many;
                ]
        }

        contained
        {
            parent[class=policy/Space]
        }
        named
        {
            parent[class=*;]
            {
                component[prefix=ep-group;
                          member=name]
            }
        }
    }
}
