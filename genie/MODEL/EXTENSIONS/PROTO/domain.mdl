module[domain]
{
    class[Config;
          concrete]
    {
        root

        dependency[config;
                   cardinality=single;
                   ]
        {
            to[class=platform/Config;
                cardinality=many;
                ]
        }

        dependency[endpointInventory;
                   cardinality=many;
                   ]
        {
            to[class=inv/LocalEndpointInventory;
                cardinality=many;
                ]
        }

        dependency[remoteEndpointInventory;
                   cardinality=single;
                   ]
        {
            to[class=inv/RemoteEndpointInventory;
                cardinality=many;
                ]
        }

        dependency[RemoteSpan;
                   cardinality=single;
                   ]
        {
            to[class=span/RemoteSpan;
                cardinality=single;
                ]
        }
        
        named
        {
            parent[class=*;]
            {
                component[prefix=domain;]
            }
        }
    }
}
