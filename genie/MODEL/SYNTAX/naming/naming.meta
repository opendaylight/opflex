metadata
{
    ref[module]
    {
        ref[class]
        {
            node[named;
                 use=module/named;
                 inherit-props=no;
                 ]
            {
                node[parent;
                     use=module/named/parent
                     ]
                {
                    node[component;
                         use=module/named/parent/component]
                }
            }
            ref[contained]
            {
                ref[parent]
                {
                    node[name-component;
                         use=module/named/parent/component]
                }
            }
        }
        ref[contained]
        {
            ref[parent]
            {
                node[name-component;
                     use=module/named/parent/component]
            }
        }

        node[named;
             use=named;
             inherit-props=no
             ]
        {
            prop[class]
            node[parent;
                 use=named/parent]
            {
                node[component;
                     use=named/parent/component]
            }
        }
    }
    ref[contained]
    {
        ref[parent]
        {
            node[name-component;
                 use=named/parent/component]
        }
    }

    node[named;
         namespace=naming;
         ]
    {
        prop[class]
        node[parent;
             namespace=naming]
        {
           prop[class]
           node[component;
                namespace=naming]
           {
                prop[prefix]
                prop[member]
           }
        }
    }
}