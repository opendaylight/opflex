[access]
	inheritFrom = All-Projects
[project]
	description = opflex
[receive]
	requireChangeId = true
[access "refs/*"]
	owner = group ldap/odl-gerrit-opflex-committers
	read = group GitHub Replication
[submit]
	action = rebase if necessary
[access "refs/heads/*"]
	create = group Release Engineering Team
	push = group Release Engineering Team
